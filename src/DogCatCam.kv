Screen: 
    name: 'DogCatCamPage'
    # on_enter:camerapreview.connect_camera(enable_analyze_pixels = True, enable_video = False, filepath_callback = camerapreview.show_toast2)
    on_enter:camerapreview.play()
    on_pre_leave:camerapreview.disconnect_camera()


    Widget:
        CameraPreview2:
            res_predict:res_predict
            res_predict_img:res_predict_img
            id:camerapreview
            size: root.size

        BoxLayout:
            orientation: 'vertical'
            size: root.size
            
            FloatLayout: 
                id:res_predict
                on_touch_down:
                    self.opacity = 0
                    # self.disabled: 'True'
                opacity: 0
                Image:
                    id:res_predict_img
                    size: root.size
                Label:
                    id:res_predict_label
                    text:camerapreview.res_predict_str

            BoxLayout:
                size_hint_y: 0.2
                Button:
                    text: "I/O"
                    background_color: 1,1,1,0
                    color: 1,.3,.3,.5
                    font_size:35
                    bold:True
                    on_press:camerapreview.play()
                Button:
                    text: "Predict"
                    background_color: 1,1,1,0
                    color: 1,.3,.3,.5
                    font_size:35
                    bold:True
                    on_press:
                        camerapreview.predict_button()
                Button:
                    text: "Predict2"
                    background_color: 1,1,1,0
                    color: 1,.3,.3,.5
                    font_size:35
                    bold:True
                    on_press:
                        camerapreview.predict_button2()