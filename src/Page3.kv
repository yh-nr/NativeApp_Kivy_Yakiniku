Screen: 
    name: 'Page3'
    on_enter:camerapreview.connect_camera(enable_analyze_pixels = True, enable_video = False)
    on_pre_leave:camerapreview.disconnect_camera()

    CameraWidget:
        CameraPreview:
            id:camerapreview
            # camera:camera
            orientation: 'vertical'
            size: root.size
        GridLayout:
            rows:4
            cols:3
            size: root.size
            # Button:
            #     text: "I/O"
            #     background_color: 1,1,1,0
            #     color: 1,1,1,1
            #     font_size:35
            #     bold:True
            #     on_press:camerapreview.play()

            Button:
                text: '[0]カルビ'
                background_color: 1,1,1,0
                color: 1,1,1,.8
                font_size:35
                bold:True
                center: root.width * 0.3, root.height * 0.3
                on_press:
                    subdir = 'Yakiniku/0'
                    camerapreview.capture_photo(subdir=subdir)

